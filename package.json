{
  "name": "qual-analyst-pro",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://Working-Futures.github.io/QualCode-Vibed",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@types/diff-match-patch": "^1.0.36",
    "diff-match-patch": "^1.0.5",
    "firebase": "^11.1.0",
    "lucide-react": "^0.292.0",
    "mammoth": "^1.6.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.5.0",
    "xlsx": "^0.18.5"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.qualcode.vibed",
    "productName": "QualCode Vibed",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "public/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    }
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-node-polyfills": "^0.25.0",
    "wait-on": "^7.2.0"
  }
}
